 <!-- Navbar scroll behavior and active section highlighting -->
 <script>
 $(document).ready(function () {
 // Navbar background change on scroll
 $(window).scroll(function () {
 if ($(window).scrollTop() > 50) {
 $(".navbar-fixed-top").addClass("top-nav-collapse");
 } else {
 $(".navbar-fixed-top").removeClass("top-nav-collapse");
 }

 // Active section highlighting
 var scrollPos = $(window).scrollTop() + 100; // offset for navbar height

 // Get all sections that have an id matching nav links
 var sections = ["#banner", "#service", "#about", "#contact"];

 sections.forEach(function (section) {
 var $section = $(section);
 if ($section.length) {
 var sectionTop = $section.offset().top;
 var sectionBottom = sectionTop + $section.outerHeight();

 if (scrollPos >= sectionTop && scrollPos < sectionBottom) {
 // Remove active from all nav items
 $(".navbar-nav li").removeClass("active");
 // Add active to matching nav item
 $('.navbar-nav li a[href="' + section + '"]')
 .parent()
 .addClass("active");
 }
 }
 });
 });

 // Set initial active state on page load
 $(".navbar-nav li").removeClass("active");
 $('.navbar-nav li a[href="#banner"]').parent().addClass("active");

 // Smooth scroll and active state on click
 $(".navbar-nav a").on("click", function (e) {
 var target = $(this).attr("href");
 if (target.charAt(0) === "#") {
 e.preventDefault();
 $("html, body").animate(
 {
 scrollTop: $(target).offset().top - 50,
 },
 800,
 "easeInOutExpo"
 );

 // Update active state
 $(".navbar-nav li").removeClass("active");
 $(this).parent().addClass("active");
 }
 });

 // Smooth scroll for hero appointment CTA (next to phone button)
 $(".banner-text .btn-appoint[href=\"#booking\"]").on("click", function (e) {
 var $target = $("#booking");
 if (!$target.length) return;
 e.preventDefault();
 var navHeight = $(".navbar-fixed-top").outerHeight() || 0;
 $("html, body").stop().animate(
 {
 scrollTop: $target.offset().top - navHeight - 10,
 },
 900,
 "easeInOutExpo"
 );
 });

 // Mobile menu open/close icon state and auto-close on link click
 var $navToggle = $(".navbar-toggle");
 $("#myNavbar").on("shown.bs.collapse", function () {
 $navToggle.addClass("is-open");
 });
 $("#myNavbar").on("hidden.bs.collapse", function () {
 $navToggle.removeClass("is-open");
 });
 $("#myNavbar a").on("click", function () {
 if ($(window).width() <= 768) {
 $("#myNavbar").collapse("hide");
 }
 });
 });
 </script>

