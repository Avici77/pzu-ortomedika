 var list = getAppointments();
 var container = document.getElementById("bookingsList");
 if (!container) return;
 if (!list.length) {
 container.innerHTML = '<p>Нема закажувања</p>';
 return;
 }
 var html = '<table class="table table-striped"><thead><tr><th>Име</th><th>Е-пошта</th><th>Тел.</th><th>Датум</th><th>Време</th><th>Услуга</th><th>Белешки</th></tr></thead><tbody>';
 list.forEach(function (a) {
 html += '<tr><td>' + escapeHtml(a.name) + '</td><td>' + escapeHtml(a.email) + '</td><td>' + escapeHtml(a.phone) + '</td><td>' + escapeHtml(a.date) + '</td><td>' + escapeHtml(a.time) + '</td><td>' + escapeHtml(a.service) + '</td><td>' + escapeHtml(a.notes) + '</td></tr>';
 });
 html += '</tbody></table>';
 container.innerHTML = html;
 }

 // Check if time has valid minute (00 or 30 only)
 function isValidTimeMinute(timeStr) {
 var timeParts = timeStr.split(':');
 var minutes = parseInt(timeParts[1], 10);
 return minutes === 0 || minutes === 30;
 }

 function getWorkingWindowByDay(dayOfWeek) {
 if (dayOfWeek === 1 || dayOfWeek === 4) return { start: 12 * 60, end: 20 * 60 };
 if (dayOfWeek === 2 || dayOfWeek === 3 || dayOfWeek === 5) return { start: 8 * 60, end: 16 * 60 };
 return null;
 }

 function formatTimeFromMinutes(totalMinutes) {
 var h = Math.floor(totalMinutes / 60);
 var m = totalMinutes % 60;
 return String(h).padStart(2, '0') + ':' + String(m).padStart(2, '0');
 }

 // Check if date/time is within working hours
 function isWithinWorkingHours(dateStr, timeStr) {
 var date = new Date(dateStr + 'T00:00:00');
 var dayOfWeek = date.getDay(); // 0=Sun, 1=Mon, ..., 6=Sat
 var window = getWorkingWindowByDay(dayOfWeek);
 if (!window) return false;

 var timeParts = timeStr.split(':');
 var hour = parseInt(timeParts[0], 10);
 var minute = parseInt(timeParts[1], 10);
 var timeInMinutes = (hour * 60) + minute;
 return isValidTimeMinute(timeStr) && timeInMinutes >= window.start && timeInMinutes <= (window.end - 30);
 }

 // Update time validation on change
 var dateInput = document.getElementById("preferredDate");
 var timeInput = document.getElementById("preferredTime");
 if (dateInput && timeInput) {
 var today = new Date();
 var yyyy = today.getFullYear();
 var mm = String(today.getMonth() + 1).padStart(2, '0');
 var dd = String(today.getDate()).padStart(2, '0');
 dateInput.min = yyyy + '-' + mm + '-' + dd;

 function renderTimeOptions(dateStr) {
 var previousValue = timeInput.value;
 timeInput.innerHTML = '';

 function appendOption(value, text, disabled, selected) {
 var opt = document.createElement('option');
 opt.value = value;
 opt.textContent = text;
 if (disabled) opt.disabled = true;
 if (selected) opt.selected = true;
 timeInput.appendChild(opt);
 }

 if (!dateStr) {
 appendOption('', 'Прво изберете датум', true, true);
 return;
 }

 var d = new Date(dateStr + 'T00:00:00');
 var window = getWorkingWindowByDay(d.getDay());

 if (!window) {
 appendOption('', 'Нема термини (неработен ден)', true, true);
 return;
 }

 appendOption('', 'Изберете време', true, true);
 var hasPrev = false;
 for (var t = window.start; t <= (window.end - 30); t += 30) {
 var value = formatTimeFromMinutes(t);
 appendOption(value, value, false, false);
 if (value === previousValue) hasPrev = true;
 }

 if (hasPrev) {
 timeInput.value = previousValue;
 }
 }

 function validateTimeSelection() {
 var date = dateInput.value;
 var time = timeInput.value;
 var msgEl = document.getElementById("timeValidationMsg");
 if (!date || !time) {
 if (msgEl) msgEl.style.display = 'none';
 return;
 }

 if (!isValidTimeMinute(time)) {
 if (msgEl) msgEl.style.display = 'block';
 if (msgEl) msgEl.textContent = 'Време мора да биде на полни или половини часови (xx:00 или xx:30)';
 } else if (date && time && !isWithinWorkingHours(date, time)) {
 if (msgEl) msgEl.style.display = 'block';
 if (msgEl) msgEl.textContent = 'Последен термин е 19:30 за смена до 20:00 (или 15:30 за смена до 16:00).';
 } else {
 if (msgEl) msgEl.style.display = 'none';
 }
 }

 renderTimeOptions(dateInput.value);
 dateInput.addEventListener("change", function () {
 renderTimeOptions(dateInput.value);
 validateTimeSelection();
 });
 timeInput.addEventListener("change", validateTimeSelection);
 }

 var form = document.getElementById("appointmentForm");
 if (form) {
 var LAST_SUBMIT_TS_KEY = 'booking_last_submit_ts';
 var SUBMIT_COOLDOWN_MS = 60 * 1000;
 var submitBtn = form.querySelector('button[type="submit"]');
 var defaultSubmitLabel = submitBtn ? submitBtn.textContent : 'Закажи';

 function setSubmitState(isSending) {
 if (!submitBtn) return;
 submitBtn.disabled = isSending;
 submitBtn.innerHTML = isSending ? '<i class="fa fa-spinner fa-spin" aria-hidden="true"></i>Испраќање...' : defaultSubmitLabel;
 }

 function setBookingMessage(msgEl, text, isError) {
 if (!msgEl) return;
 msgEl.textContent = text;
 msgEl.style.color = isError ? '#c0392b' : '#1f7f67';
 }

 form.addEventListener("submit", function (e) {
 e.preventDefault();
 var now = Date.now();
 var lastSubmitTs = parseInt(localStorage.getItem(LAST_SUBMIT_TS_KEY) || '0', 10);
 var elapsed = now - lastSubmitTs;
 var remainingMs = SUBMIT_COOLDOWN_MS - elapsed;

 var name = document.getElementById("fullName").value.trim();
 var email = document.getElementById("email").value.trim();
 var phone = document.getElementById("phone").value.trim();
 var date = document.getElementById("preferredDate").value;
 var timeInputEl = document.getElementById("preferredTime");
 var time = timeInputEl ? timeInputEl.value : '';
 var serviceType = document.getElementById("serviceType").value;
 var notes = document.getElementById("notes").value.trim();
 var honeypot = document.getElementById("websiteField");

 var msgEl = document.getElementById("bookingMessage");

 if (honeypot && honeypot.value.trim() !== '') {
 setBookingMessage(msgEl, "Неуспешно испраќање. Обидете се повторно.", true);
 return;
 }

 if (remainingMs > 0) {
 var remainingSec = Math.ceil(remainingMs / 1000);
 setBookingMessage(msgEl, 'Ве молиме почекајте ' + remainingSec + ' сек. пред повторно закажување.', true);
 return;
 }

 if (!name || !email || !phone || !date || !time) {
 setBookingMessage(msgEl, "Пополнете ги сите обавезни полиња.", true);
 return;
 }

 if (dateInput && dateInput.min && date < dateInput.min) {
 setBookingMessage(msgEl, "Не може да изберете минат датум.", true);
 return;
 }

 // Validate time is within working hours
 if (!isWithinWorkingHours(date, time)) {
 setBookingMessage(msgEl, "Време надвор од работно време. Ве молиме изберете време за време на работа.", true);
 return;
 }

 // Validate time minutes are 00 or 30
 if (!isValidTimeMinute(time)) {
 setBookingMessage(msgEl, "Време мора да биде на полни или половини часови (xx:00 или xx:30).", true);
 return;
 }

 setSubmitState(true);
 setBookingMessage(msgEl, 'Се испраќа закажувањето...', false);

 var appt = { id: Date.now(), name: name, email: email, phone: phone, date: date, time: time, service: serviceType, notes: notes, created: new Date().toISOString() };
 var appts = getAppointments();
 appts.push(appt);
 saveAppointments(appts);

 // Try to send via EmailJS if configured, otherwise fallback to mailto
 if (window.emailjs && EMAILJS_SERVICE_ID !== 'YOUR_SERVICE_ID' && EMAILJS_TEMPLATE_ID !== 'YOUR_TEMPLATE_ID' && EMAILJS_PUBLIC_KEY !== 'YOUR_PUBLIC_KEY') {
 try {
 emailjs.init({ publicKey: EMAILJS_PUBLIC_KEY });
 var templateParams = {
 to_email: 'pzuortomedika@gmail.com',
 to_name: 'ПЗУ Ортомедика',
 name: name,
 from_name: name,
 email: email,
 from_email: email,
 reply_to: email,
 phone: phone,
 date: date,
 time: time,
 service: serviceType,
 notes: notes,
 subject: 'Ново закажување: ' + name + ' - ' + date + ' ' + time,
 message: 'Име: ' + name + '\nЕ-пошта: ' + email + '\nТелефон: ' + phone + '\nДатум: ' + date + '\nВреме: ' + time + '\nУслуга: ' + serviceType + '\nБелешки: ' + notes
 };
 emailjs.send(EMAILJS_SERVICE_ID, EMAILJS_TEMPLATE_ID, templateParams)
 .then(function () {
 localStorage.setItem(LAST_SUBMIT_TS_KEY, String(Date.now()));
 setBookingMessage(msgEl, 'Закажувањето е успешно испратено. Ќе ве контактираме наскоро.', false);
 form.reset();
 setSubmitState(false);
 }, function (err) {
 console.error('EmailJS error', err);
 var detail = (err && (err.text || err.message || err.status)) ? (' (' + (err.text || err.message || err.status) + ')') : '';
 setBookingMessage(msgEl, 'Грешка при праќање на е-пошта. Обидете се повторно.' + detail, true);
 setSubmitState(false);
 });
 } catch (err) {
 console.error(err);
 setBookingMessage(msgEl, 'Грешка при праќање на е-пошта. ' + (err && err.message ? err.message : ''), true);
 setSubmitState(false);
 }
 } else {
 localStorage.setItem(LAST_SUBMIT_TS_KEY, String(Date.now()));
 setBookingMessage(msgEl, 'Закажувањето е зачувано локално. Отворете ваш поштенски клиент за потврда.', false);
 var subject = encodeURIComponent('Закажување - ' + name + ' ' + date + ' ' + time);
 var body = encodeURIComponent('Име: ' + name + '\nЕ-пошта: ' + email + '\nТелефон: ' + phone + '\nДатум: ' + date + '\nВреме: ' + time + '\nУслуга: ' + serviceType + '\nБелешки: ' + notes);
 window.location.href = 'mailto:pzuortomedika@gmail.com?subject=' + subject + '&body=' + body;
 form.reset();
 setSubmitState(false);
 }
 });
 }

 var viewBtn = document.getElementById("viewBookingsBtn");
 if (viewBtn) {
 viewBtn.addEventListener("click", function () {
 var admin = document.getElementById("bookingsAdmin");
 admin.style.display = admin.style.display === "none" ? "block" : "none";
 renderBookings();
 });
 }

 var exportBtn = document.getElementById("exportCsvBtn");
 if (exportBtn) {
 exportBtn.addEventListener("click", function () {
 var rows = getAppointments();
 if (!rows.length) {
 alert("Нема закажувања");
 return;
 }
 var csv = 'Име,Е-пошта,Телефон,Датум,Време,Услуга,Белешки\n';
 rows.forEach(function (r) {
 csv += [r.name, r.email, r.phone, r.date, r.time, r.service, r.notes].map(function (v) { return '"' + String(v || '').replace(/"/g, '""') + '"'; }).join(',') + '\n';
 });
 var blob = new Blob([csv], { type: 'text/csv;charset=utf-8;' });
 var url = URL.createObjectURL(blob);
 var a = document.createElement('a');
 a.href = url;
 a.download = 'appointments.csv';
 document.body.appendChild(a); a.click(); a.remove();
 URL.revokeObjectURL(url);
 });
 }

 var clearBtn = document.getElementById("clearBookingsBtn");
 if (clearBtn) {
 clearBtn.addEventListener("click", function () {
 if (!confirm('Дали сте сигурни дека сакате да ги избришете сите закажувања?')) return;
 localStorage.removeItem('appointments');
 renderBookings();
 alert('Закажувањата се избришани.');
 });
 }
 })();
 </script>
 </body>
</html>
